{{#each section}}
	<section class="category">
		<h1>{{this.id}}</h1>
		<div class="items">
		{{#each this.item}}
			<section class="item" id="{{this.id}}">
				<a href="{{this.url}}">
					<img src="{{this.image}}" alt="{{this.id}}" />
					<h2>{{this.title}}</h2>
				</a>
			</section>
		{{/each}}
		</div>
	</section>
{{/each}}
